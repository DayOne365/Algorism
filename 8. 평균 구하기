class Solution {
    public double solution(int[] arr) {
        double answer = 0;
        double sum = 0;
        // 1 + 2 + 3 + 4 = 10 / 4
        // 배열 수를 알아내는 법 
        
        for (int i = 0; i < arr.length; i++){
           sum += arr[i] ;   
        }
        answer += sum / arr.length;
        return answer;
    }
}
